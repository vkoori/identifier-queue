{
    "name": "vkoori/identifier-queue",
    "description": "This package is created for dispatching queues that have a identifier field for searching.",
    "keywords": [
        "lumen",
        "queue"
    ],
    "type": "library",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "laravel/lumen-framework": "^9.0|^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Kooriv\\Queue\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kooriv\\Queue\\Providers\\QueueServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
